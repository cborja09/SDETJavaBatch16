package Class19;

//Create 1 class with a private method that has 3 overloaded forms.
//Then call each overloaded method with specific arguments and observe result.

class PrivateMethod {

    private void Method(String word, int num){
        System.out.println("The first part of " + " " +word+ " " +num);
    }

    private void Method(int num){
        System.out.println("The second part of " + " " +num);
    }
    private void Method(String word){
        System.out.println("The third part of " + " " +word);
    }

    public static void main(String[] args) {
        PrivateMethod priv = new PrivateMethod();
        priv.Method("January", 9);
        priv.Method(9);
        priv.Method("January");
    }
}
